{
  "manifest_version": 3,
  "name": "Nomi",
  "version": "1.0.0",
  "description": "Capture thoughts and organize them into actionable calendar events",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3001/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}

